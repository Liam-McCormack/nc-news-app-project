te;\n    const { loggedInUser } = this.context;\n    if (isLoading) {\n      return (\n        <div className=\"loading\">\n          <ReactLoading type={\"bars\"} color={\"grey\"} />\n        </div>\n      );\n    } else {\n      return (\n        <section>\n          {this.props.topic ? (\n            <h2>All Articles in {this.props.topic.toUpperCase()}:</h2>\n          ) : (\n            <h2>All Articles:</h2>\n          )}\n          <ul className=\"list\">\n            {articles.map((article) => {\n              return (\n                <>\n                  <ArticleCard key={article.article_id} {...article} />\n                  {loggedInUser === article.author && (\n                    <Delete\n                      article_id={article.article_id}\n                      deleteOnClick={this.deleteOnClick}\n                    />\n                  )}\n                </>\n              );\n            })}\n          </ul>\n        </section>\n      );\n    }\n  }\n}\n\nArticles.contextType = UserContext;\n\nexport default Articles;\n","/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/Topics.jsx",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/Users.jsx",["101"],"import React from \"react\";\nimport { getUsers } from \"../api\";\nimport UserCard from \"./UserCard\";\nimport ReactLoading from \"react-loading\";\nimport { getSingleUser } from \"../api\";\n\nclass Users extends React.Component {\n  state = {\n    users: [],\n    isLoading: true,\n  };\n\n  componentDidMount() {\n    getUsers().then((users) => {\n      this.setState({ users, isLoading: false });\n    });\n  }\n\n  render() {\n    const { users, isLoading } = this.state;\n    if (isLoading) {\n      return (\n        <div className=\"loading\">\n          <ReactLoading type={\"bars\"} color={\"grey\"} />\n        </div>\n      );\n    } else {\n      return (\n        <section>\n          <button>Create User</button>\n          <ul className=\"user-list\">\n            {users.map((user) => {\n              return <UserCard key={user.username} {...user} />;\n            })}\n          </ul>\n        </section>\n      );\n    }\n  }\n}\n\nexport default Users;\n","/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/api.js",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/ArticleCard.jsx",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/SingleArticle.jsx",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/TopicCard.jsx",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/UserCard.jsx",["102"],"import React from \"react\";\nimport { Link } from \"@reach/router\";\n\nconst UserCard = (props) => {\n  const user = props;\n  return (\n    <div className=\"userCard\">\n      <img src={user.avatar_url} className=\"user-avatar\" />\n      <Link to={`/users/${user.username}`}>\n        <h2>{user.username}</h2>\n      </Link>\n    </div>\n  );\n};\n\nexport default UserCard;\n","/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/UserProfile.jsx",["103"],"import React from \"react\";\nimport { getSingleUser } from \"../api\";\nimport ReactLoading from \"react-loading\";\n\nclass UserProfile extends React.Component {\n  state = {\n    user: {\n      username: \"tickle122\",\n      avatar_url:\n        \"https://vignette.wikia.nocookie.net/mrmen/images/d/d6/Mr-Tickle-9a.png/revision/latest?cb=20180127221953\",\n      name: \"Tom Tickle\",\n    },\n    isLoading: true,\n  };\n\n  componentDidMount() {\n    const username = this.props.username;\n    getSingleUser(username).then((user) => {\n      this.setState({ user, isLoading: false });\n    });\n  }\n\n  render() {\n    const { user, isLoading } = this.state;\n    if (isLoading) {\n      return (\n        <div className=\"loading\">\n          <ReactLoading type={\"bars\"} color={\"grey\"} />\n        </div>\n      );\n    } else {\n      return (\n        <div>\n          <p>Avatar:</p>\n          <img src={user.avatar_url} className=\"user-avatar\" />\n          <h2>Username: {user.username}</h2>\n          <p>Name: {user.name}</p>\n        </div>\n      );\n    }\n  }\n}\n\nexport default UserProfile;\n","/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/ArticlesByTopic.jsx",["104"],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/Comments.jsx",["105","106","107"],"import React from \"react\";\nimport { getComments, sendComment } from \"../api\";\nimport CommentCard from \"./CommentCard\";\nimport ReactLoading from \"react-loading\";\nimport CommentAdder from \"./PostComment\";\nimport { useContext } from \"react\";\nimport { UserContext } from \"../contexts/User\";\nimport Delete from \"./Delete\";\n\nclass CommentSection extends React.Component {\n  state = {\n    comments: [],\n    isLoading: true,\n    addComment: false,\n  };\n\n  componentDidMount() {\n    const id = this.props.article_id;\n    getComments(id).then((comments) => {\n      this.setState({ comments, isLoading: false });\n    });\n  }\n\n  postComment = (commentObj) => {\n    const id = this.props.article_id;\n    sendComment(id, commentObj).then((newComment) => {\n      this.setState((currentState) => {\n        return {\n          comments: [newComment, ...currentState.comments],\n        };\n      });\n    });\n  };\n\n  addComment = () => {\n    if (this.state.addComment) this.setState({ addComment: false });\n    else this.setState({ addComment: true });\n  };\n\n  render() {\n    const { comments, isLoading } = this.state;\n    const { loggedInUser } = this.context;\n    if (isLoading) {\n      return (\n        <div className=\"loading\">\n          <ReactLoading type={\"bars\"} color={\"grey\"} />\n        </div>\n      );\n    } else {\n      return (\n        <section>\n          {this.state.addComment ? (\n            <button onClick={this.addComment}>Stop Commenting</button>\n          ) : (\n            <button onClick={this.addComment}>Add Comment</button>\n          )}\n\n          {this.state.addComment ? (\n            <CommentAdder postComment={this.postComment} />\n          ) : null}\n\n          <ul className=\"list\">\n            {comments.map((comment) => {\n              return (\n                <>\n                  <CommentCard key={comment.comment_id} {...comment} />\n                  {/* <Delete\n                    comment_id={comment.comment_id}\n                    article_id={this.props.article_id}\n                    deleteOnClick={this.deleteOnClick}\n                  /> */}\n                </>\n              );\n            })}\n          </ul>\n        </section>\n      );\n    }\n  }\n}\n\nCommentSection.contextType = UserContext;\n\nexport default CommentSection;\n","/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/CommentCard.jsx",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/Login.jsx",["108","109","110","111"],"import React from \"react\";\n\nclass Login extends React.Component {\n  state = {\n    username: \"\",\n    users: [\n      \"tickle122\",\n      \"grumpy19\",\n      \"happyamy2016\",\n      \"cooljmessy\",\n      \"weegembump\",\n      \"jessjelly\",\n    ],\n    badAttempt: 0,\n    recommendUser: \"\",\n  };\n\n  handleChange = (event) => {\n    const { value, name } = event.target;\n    this.setState({ [name]: value });\n  };\n\n  handleClick = () => {\n    const { login } = this.props;\n    const { users, username } = this.state;\n    if (users.includes(username)) login(username);\n    else {\n      this.setState((currentState) => {\n        let user = \"\";\n\n        if (currentState.badAttempt > 0) user = currentState.recommendUser;\n        else {\n          user = users[Math.floor(Math.random() * users.length)];\n        }\n\n        const newState = {\n          badAttempt: currentState.badAttempt + 1,\n          recommendUser: user,\n        };\n        return newState;\n      });\n    }\n  };\n\n  render() {\n    const { user, login, logout } = this.props;\n    const { users } = this.state;\n    return (\n      <section className=\"login-container\">\n        <div className=\"login-body\">\n          <h1>NORTHCODERS NEWS</h1>\n          <h2>\n            Welcome to Northcoders News! The home of news that may or may not\n            actually be news!\n          </h2>\n          {/* maybe change to a form to allow the Enter press? */}\n          <br></br>\n          <label for=\"username\">Please login below:</label>\n          <br></br>\n          <input\n            type=\"text\"\n            name=\"username\"\n            placeholder=\"Username\"\n            onChange={this.handleChange}\n          />\n          <button onClick={this.handleClick}>Login</button>\n\n          {this.state.badAttempt >= 1 ? (\n            <p>Psst...try '{this.state.recommendUser}'!</p>\n          ) : null}\n        </div>\n      </section>\n    );\n  }\n}\n\nexport default Login;\n","/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/PostComment.jsx",[],["112","113"],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/contexts/User.js",[],"/home/liammc/northcoders/frontend/nc-news-app/nc-news-app/src/components/Delete.jsx",["114"],"import React from \"react\";\n\nconst Delete = (props) => {\n  const { article_id, deleteOnClick, comment_id } = props;\n\n  //   const deleteOnClick = () => {\n  //     console.log(article_id);\n  //     deleteArticle(article_id).then((data) => {\n  //       console.log(data);\n  //     });\n  //   };\n\n  return (\n    <div>\n      <button onClick={() => deleteOnClick(article_id)}>\n        Delete Your Article\n      </button>\n    </div>\n  );\n};\n\nexport default Delete;\n",{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":2,"column":17,"nodeType":"121","messag